{
    "name": "codegreencreative/laravel-samlidp",
    "description": "Make your Laravel application an Idenification Provider using SAML 2.0.",
    "keywords": ["laravel", "saml", "saml 2.0", "auth", "acl", "sso", "idp"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0",
        "illuminate/routing": "^7.0",
        "lightsaml/lightsaml": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "CodeGreenCreative\\SamlIdp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeGreenCreative\\SamlIdp\\LaravelSamlIdpServiceProvider"
            ]
        }
    },
    "repositories": [
        { "type": "package",
            "package": {
              "name": "lightsaml/lightsaml",  
              "version": "1.6",
              "source": {
                "url": "https://github.com/NETFORMIC-GmbH/lightSAML.git", 
                "type": "git", 
                "reference": "origin/master"
              }
            }
          }

      ]
}
